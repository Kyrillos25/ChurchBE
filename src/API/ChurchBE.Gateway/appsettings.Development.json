{
  "Authentication": {
    "Audience": "account",
    "TokenValidationParameters": {
      "ValidIssuers": [ "http://churchbe.identity:8080/realms/churchbe", "http://localhost:18080/realms/churchbe" ]
    },
    "MetadataAddress": "http://churchbe.identity:8080/realms/churchbe/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://churchbe.seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "churchbe.Gateway"
    }
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://churchbe.jaeger:4317",
  "ReverseProxy": {
    "Routes": {
      "ChurchBE-Users-route1": {
        "ClusterId": "ChurchBE-Users-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "users/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "user/{**catch-all}" } ]
      },
      "ChurchBE-Users-route2": {
        "ClusterId": "ChurchBE-Users-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "users/login"
        }
      },
      "ChurchBE-Users-route3": {
        "ClusterId": "ChurchBE-Users-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "users/register"
        }
      }
    },
    "Clusters": {
      "churchbe-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://churchbe.api:8080"
          }
        }
      }
    }
  },
  "Users": {
    "Outbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 20
    },
    "Inbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 20
    },
    "KeyCloak": {
      "AdminUrl": "http://ChurchBE.identity:8080/admin/realms/ChurchBE/",
      "TokenUrl": "http://ChurchBE.identity:8080/realms/ChurchBE/protocol/openid-connect/token",
      "ConfidentialClientId": "ChurchBE-API",
      "ConfidentialClientSecret": "6rOdVfMh6PunwlWE0JOoRYmyoKczWsSp",
      "PublicClientId": "ChurchBE-SPA"
    }
  }
}
