{
  "ConnectionStrings": {
    "Database": "Host=churchbe.database;Port=5432;Database=churchbe;Username=postgres;Password=postgres;Include Error Detail=true",
    "Cache": "ChurchBE.redis:6380",
    "Queue": "amqp://ChurchBE-queue:5672"
  },
  "Authentication": {
    "Audience": "account",
    "TokenValidationParameters": {
      "ValidIssuers": [ "http://ChurchBE.identity:8080/realms/ChurchBE", "http://localhost:18080/realms/ChurchBE" ]
    },
    "MetadataAddress": "http://ChurchBE.identity:8080/realms/ChurchBE/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "KeyCloak": {
    "HealthUrl": "http://ChurchBE.identity:8080/health/"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "ChurchBE.Modules.Users.Infrastructure.Outbox": "Warning",
        "ChurchBE.Modules.Users.Infrastructure.Inbox": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://ChurchBE.seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ChurchBE.Users.Api"
    }
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://ChurchBE.jaeger:4317",
  "Users": {
    "Outbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 20
    },
    "Inbox": {
      "IntervalInSeconds": 5,
      "BatchSize": 20
    },
    "KeyCloak": {
      "AdminUrl": "http://ChurchBE.identity:8080/admin/realms/ChurchBE/",
      "TokenUrl": "http://ChurchBE.identity:8080/realms/ChurchBE/protocol/openid-connect/token",
      "ConfidentialClientId": "ChurchBE-API",
      "ConfidentialClientSecret": "6rOdVfMh6PunwlWE0JOoRYmyoKczWsSp",
      "PublicClientId": "ChurchBE-SPA"
    }
  }
}